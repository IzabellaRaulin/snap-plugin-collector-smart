{
  "version": 1,
  "schedule": {
    "type": "simple",
    "interval": "5s"
  },
  "workflow": {
    "collect": {
      "metrics": {
        "/intel/disk/smart/*/reallocatedsectors": {},
        "/intel/disk/smart/*/reallocatedsectors/normalized": {},
        "/intel/disk/smart/*/reservedblocks": {},
        "/intel/disk/smart/*/reservedblocks/normalized": {},
        "/intel/disk/smart/*/satadownshifts": {},
        "/intel/disk/smart/*/satadownshifts/normalized":{},
        "/intel/disk/smart/*/thermalthrottle":{},
        "/intel/disk/smart/*/thermalthrottle/eventcount":{}
      },
      "config": {
	"/intel/disk/smart": {
	  "proc_path": "/proc",
	  "dev_path": "/dev"
	}
      },
      "process": null,
      "publish": [
          {
              "plugin_name": "mock-file",
              "config": {
                  "file": "/tmp/published_smart.log"
              }
          }
      ]
    }
  }
}
